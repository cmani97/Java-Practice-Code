package leetcode;

public class All {
	
	public int noOfIslands(int[][] grid) {
		int max = 0;
		
		
		return max;
	}
	public int I(int[][] arr, int i, int j) {
		if(i>=arr.length || j>=arr.length)
			return 0;
		if(arr[i][j]==1)
			return 1;
		
		i(arr, i-1, j);
		i(arr, i+1, j);
		i(arr, i, j-1);
		i(arr, i, j+1);
		
	}
	/* */
}
